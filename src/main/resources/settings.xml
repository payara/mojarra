<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>${maven.repo.local}</localRepository>
    <mirrors>
        <mirror>
            <id>central-https</id>
            <name>Maven Central HTTPS</name>
            <url>https://repo.maven.apache.org/maven2</url>
            <mirrorOf>central,maven2-repository.dev.java.net,glassfish-repository,java.net.Releases</mirrorOf>
        </mirror>
    </mirrors>
</settings>
